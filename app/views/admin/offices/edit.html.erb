<% provide :title, "Edit Office" %>
<% provide :button_text, "Update" %>

<div class="col-md-6 col-md-offset-3">
  <div>
    <%= link_to admin_offices_path, class: "btn btn-info btn-xs", title: "Go back" do %>
      <span class="glyphicon glyphicon-arrow-left"></span> All offices
    <% end %>

    <%= link_to admin_office_path(@office), class: "btn btn-default btn-xs pull-right", title: "View office" do %>
      <span class="glyphicon glyphicon-eye-open"></span>
    <% end %>
  </div>
  <h2 class="text-center">Edit Office</h2>

  <%= render "form_office", object: @office %>
</div>
